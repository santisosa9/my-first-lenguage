Program {
    int sumatory(int x) {
        int i = 0;
        int result = 0;
        while (i < x || i == x) {
            result = result + i;
            i = i + 1;
        }
        return result;
    }

    void print_int(int x) extern;

    void main () {
        int sum = sumatory(2);
        print_int(sum);
    }
}

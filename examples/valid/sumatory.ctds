Program {
    void print_int(int x) extern;

    int sumatory(int x) {
        int i = 0;
        int result = 0;
        print_int(x);
        int limit = x;
        while (i < limit || i == limit) {
            result = result + i;
            print_int(i);
            i = i + 1;
        }
        return result;
    }


    void main () {
        int x = 10;
        print_int(x);
        print_int(x * 2);
        print_int(x / 2);
        print_int(x % 2);
        int sum = sumatory(x);
        print_int(sum);
    }
}

Program {
    void print_int(int x) extern;

    int sumatory(int x) {
        int i = 0;
        int result = 0;
        int limit = 5;
        while (i < limit || i == limit) {
            result = result + i;
            print_int(i);
            i = i + 1;
        }
        return result;
    }


    void main () {
        int sum = sumatory(2);
        print_int(sum);
    }
}
